<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>

<AnchorPane xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.elms.controller.BorrowingHistoryController"
            prefHeight="400.0" prefWidth="600.0">

    <TableView fx:id="transactionTable" layoutY="50.0" layoutX="25.0" prefHeight="300.0" prefWidth="550.0">
        <columns>
            <TableColumn fx:id="idColumn" text="ID" prefWidth="100.0"/>
            <TableColumn fx:id="borrowDateColumn" text="Borrow Date" prefWidth="100.0"/>
            <TableColumn fx:id="returnDateColumn" text="Return Date" prefWidth="100.0"/>
            <TableColumn fx:id="studentIdColumn" text="Student ID" prefWidth="100.0"/>
            <TableColumn fx:id="bookIdColumn" text="Book ID" prefWidth="100.0"/>
        </columns>
    </TableView>

</AnchorPane>